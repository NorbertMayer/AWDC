    <articlesMain id="1094" key="4a087583-1c46-478d-9ef9-d9c17738ece4" parentID="1093" level="2" creatorID="0" sortOrder="0" createDate="2018-09-19T11:07:46" updateDate="2018-09-19T11:07:46" nodeName="Articles" urlName="articles" path="-1,1093,1094" isDoc="" nodeType="1076" creatorName="Norbert Mayer" writerName="Norbert Mayer" writerID="0" template="1075" nodeTypeAlias="articlesMain" isPublished="true">
